{
  "headers": [
    {
      "source": "/api/status/(.*)",
      "headers": [
        {
          "key": "access-control-allow-origin",
          "value": "*"
        },
        { "key": "access-control-allow-methods", "value": "GET" }
      ]
    },
    {
      "source": "/s/(.*)",
      "headers": [
        {
          "key": "access-control-allow-origin",
          "value": "*"
        },
        { "key": "access-control-allow-methods", "value": "GET" }
      ]
    }
  ]
}
